{
  "name": "merge-org/dummy-product-cli-app",
  "version": "0.3.0",
  "type": "app",
  "autoload": {
    "MergeOrg\\DummyProductCliApp\\": "src",
    "Tests\\": "tests"
  },
  "require": {
    "php": "^7.4||^8.0||^8.1||^8.2||^8.3",
    "automattic/woocommerce": "^3",
    "adhocore/cli": "^1",
    "fakerphp/faker": "^1",
    "mbezhanov/faker-provider-collection": "^2"
  },
  "require-dev": {
    "pestphp/pest": "^2",
    "phpstan/phpstan": "^1",
    "roave/backward-compatibility-check": "=8.7.0",
    "marcocesarato/php-conventional-changelog": "^1"
  },
  "autoload-dev": {
    "psr-4": {
      "MergeOrg\\DummyProductCliApp\\": "src",
      "Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
